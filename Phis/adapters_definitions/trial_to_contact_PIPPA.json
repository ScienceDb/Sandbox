{
    "model": "trial_to_contact",
    "storageType": "ddm-adapter",
    "adapterName": "trial_to_contact_PIPPA",
    "regex": "pippa",
    "url": "http://pippa-graphql-container:3002/graphql",
    "attributes":{
        "trialDbId": "String",
        "contactDbId": "String"
    },
    "associations": {
        "trial": {
            "type": "to_one",
            "target": "trial",
            "targetKey": "trialDbId",
            "keyIn": "trial_to_contact",
            "targetStorageType": "distributed-data-model",
            "label": "trialName"
        },
        "contact": {
            "type": "to_one",
            "target": "contact",
            "targetKey": "contactDbId",
            "keyIn": "trial_to_contact",
            "targetStorageType": "distributed-data-model",
            "label": "name"
        }
    }
}