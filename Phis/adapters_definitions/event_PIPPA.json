{
    "model": "event",
    "storageType": "ddm-adapter",
    "adapterName": "event_PIPPA",
    "regex": "pippa",
    "url": "http://pippa-graphql-container:3002/graphql",
    "attributes": {
        "eventDbId": "String",
        "eventDescription": "String",
        "eventType": "String",
        "studyDbId": "String",

        "date": "DateTime"

    },
    "associations": {
        "eventParameters": {
            "type": "to_many",
            "target": "eventParameter",
            "targetKey": "eventDbId",
            "keyIn": "eventParameter",
            "targetStorageType": "distributed-data-model",
            "label": "key"
        },
        "eventToObservationUnits": {
            "type": "to_many",
            "target": "observationUnit_to_event",
            "targetKey": "eventDbId",
            "keyIn": "observationUnit_to_event",
            "targetStorageType": "distributed-data-model"
        },
        "study": {
            "type": "to_one",
            "target": "study",
            "targetKey": "studyDbId",
            "keyIn": "event",
            "targetStorageType": "distributed-data-model",
            "label": "studyName",
            "sublabel": "studyDbId"
        }
    },
    "internalId": "eventType"
}
