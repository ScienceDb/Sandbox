{
    "model": "program",
    "storageType": "ddm-adapter",
    "adapterName": "program_PIPPA",
    "regex": "pippa",
    "url": "http://pippa-graphql-container:3002/graphql",
    "attributes": {
        "abbreviation": "String",
        "commonCropName": "String",
        "documentationURL": "String",
        "leadPersonDbId": "String",
        "leadPersonName": "String",
        "objective": "String",
        "programName": "String",
        "programDbId": "String"
    },
    "associations" : {
        "trials" : {
            "type": "to_many",
            "target": "trial",
            "targetKey": "programDbId",
            "keyIn": "trial",
            "targetStorageType": "distributed-data-model",
            "label": "trialName"
        },
        "observationUnits": {
            "type": "to_many",
            "target": "observationUnit",
            "targetKey": "programDbId",
            "keyIn": "observationUnit",
            "targetStorageType": "distributed-data-model",
            "label": "observationUnitName"
        }
    },
    "internalId": "programDbId"
}
