{
    "model": "observationUnit_to_event",
    "storageType": "ddm-adapter",
    "adapterName": "observationUnit_to_event_PIPPA",
    "regex": "pippa",
    "url": "http://pippa-graphql-container:3002/graphql",
    "attributes":{
        "observationUnitDbId": "String",
        "eventDbId": "String"
    },
    "associations": {
        "observationUnit": {
            "type": "to_one",
            "target": "observationUnit",
            "targetKey": "observationUnitDbId",
            "keyIn": "observationUnit_to_event",
            "targetStorageType": "distributed-data-model",
            "label": "observationUnitName"
        },
        "event": {
            "type": "to_one",
            "target": "event",
            "targetKey": "eventDbId",
            "keyIn": "observationUnit_to_event",
            "targetStorageType": "distributed-data-model",
            "label": "eventType"
        }
    }
}