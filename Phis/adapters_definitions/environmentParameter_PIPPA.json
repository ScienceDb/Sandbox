{
    "model": "environmentParameter",
    "storageType": "ddm-adapter",
    "adapterName": "environmentParameter_PIPPA",
    "regex": "pippa",
    "url": "http://pippa-graphql-container:3002/graphql",
    "attributes": {
        "description": "String",
        "parameterName": "String",
        "parameterPUI": "String",
        "unit": "String",
        "unitPUI": "String",
        "value": "String",
        "valuePUI": "String",

        "studyDbId": "String",
        "environmentParameterDbId": "String"
    },
    "associations": {
        "study": {
            "type": "to_one",
            "target": "study",
            "targetKey": "studyDbId",
            "keyIn": "environmentParameter",
            "targetStorageType": "distributed-data-model",
            "label": "studyName"
        }
    },
    "internalId": "environmentParameterDbId"
}
