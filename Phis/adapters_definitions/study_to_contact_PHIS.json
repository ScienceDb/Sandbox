{
    "model": "study_to_contact",
    "storageType": "sql-adapter",
    "adapterName": "study_to_contact_PHIS",
    "regex": "phis",
    
    "attributes":{
        "studyDbId": "String",
        "contactDbId": "String"
    },
    "associations": {
        "study": {
            "type": "to_one",
            "target": "study",
            "targetKey": "studyDbId",
            "keyIn": "study_to_contact",
            "targetStorageType": "distributed-data-model",
            "label": "studyName"
        },
        "contact": {
            "type": "to_one",
            "target": "contact",
            "targetKey": "contactDbId",
            "keyIn": "study_to_contact",
            "targetStorageType": "distributed-data-model",
            "label": "name"
        }
    }
}