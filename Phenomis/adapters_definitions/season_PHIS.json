{
    "model": "season",
    "storageType": "ddm-adapter",
    "adapterName": "season_PHIS",
    "regex": "phis",
    "url": "http://phis-graphql-container:3001/graphql",
    "attributes": {
        "season": "String",
        "seasonDbId": "String",
        "year": "Int"
    },
    "associations": {
        "observations": {
            "type": "to_many",
            "target": "observation",
            "targetKey": "seasonDbId",
            "keyIn": "observation",
            "targetStorageType": "distributed-data-model",
            "label": "observationVariableName",
            "sublabel" : "value"
        },
        "seasonToStudies": {
            "type": "to_many",
            "target": "study_to_season",
            "targetKey": "seasonDbId",
            "keyIn": "study_to_season",
            "targetStorageType": "distributed-data-model"
        }
    },
    "internalId": "seasonDbId"
}
