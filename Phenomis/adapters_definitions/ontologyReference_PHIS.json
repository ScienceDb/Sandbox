{
    "model": "ontologyReference",
    "storageType": "cenzontle-webservice-adapter",
    "adapterName": "ontologyReference_PHIS",
    "regex": "phis",
    "url": "http://phis-graphql-container:3001/graphql",
    "attributes": {
        "documentationURL": "String",
        "ontologyDbId": "String",
        "ontologyName": "String",
        "version": "String"
    },
    "associations": {
        "scales" : {
            "type": "to_many",
            "target": "scale",
            "targetKey": "ontologyDbId",
            "keyIn": "scale",
            "targetStorageType": "distributed-data-model",
            "label": "scaleName"
        },
        "methods" : {
            "type": "to_many",
            "target": "method",
            "targetKey": "ontologyDbId",
            "keyIn": "method",
            "targetStorageType": "distributed-data-model",
            "label": "methodName"
        },
        "traits" : {
            "type": "to_many",
            "target": "trait",
            "targetKey": "ontologyDbId",
            "keyIn": "trait",
            "targetStorageType": "distributed-data-model",
            "label": "traitName"
        },
        "observationVariables" : {
            "type": "to_many",
            "target": "observationVariable",
            "targetKey": "ontologyDbId",
            "keyIn": "observationVariable",
            "targetStorageType": "distributed-data-model",
            "label": "ObservationVariableName"
        }
    },
    "internalId": "ontologyDbId"
}
