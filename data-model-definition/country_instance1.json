{
    "model": "country",
    "storageType": "sql-adapter",
    "adapterName": "country_instance1",
    "regex": "instance1",
    "attributes": {
        "country_id":"String",
        "name": "String",
        "book_ids": "[String]"
    },
  
    "associations": {
        "capital": {
            "type": "one_to_one",
            "implementation": "foreignkeys",
            "reverseAssociation": "country",
            "target": "capital",
            "targetKey": "country_id",
            "keysIn": "capital",
            "targetStorageType": "distributed-data-model"
        },
        "available_books":{
            "type":"many_to_many",
            "implementation": "foreignkeys",
            "reverseAssociation": "countries",
            "target":"book",
            "targetKey": "country_ids",
            "sourceKey": "book_ids",
            "keysIn": "country",
            "targetStorageType": "distributed-data-model"
        }
  
    },
  
    "internalId": "country_id",
    "useDataLoader": false
}